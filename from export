    .sr-plate .sr-plate__picture {
        position: absolute;
        bottom: -15px;
        left: calc(50% - 30px);
        width: 60px;
    }
    @media screen and (min-width: 1024px) and (max-width: 1439px) {
        .sr-plate-wrapper,
        .sr-plate,
        .sr-plate .sr-plate__content {
            width: 60px;
            height: 60px;
        }

        .sr-plate .sr-plate__text {
            font-size: 11px;
            line-height: 12px;
        }

        .sr-plate .sr-plate__picture {
            bottom: -10px;
            left: calc(50% - 22px);
            width: 44px;
        }
    }
    @media screen and (min-width: 768px) and (max-width: 1023px) {
        .sr-plate-wrapper,
        .sr-plate,
        .sr-plate .sr-plate__content {
            width: 40px;
            height: 40px;
        }

        .sr-plate .sr-plate__text {
            font-size: 7px;
            line-height: 8px;
        }

        .sr-plate .sr-plate__picture {
            bottom: -6px;
            left: calc(50% - 15px);
            width: 30px;
        }
    }
    @media screen and (max-width: 479px) {
        .sr-plate-wrapper,
        .sr-plate,
        .sr-plate .sr-plate__content {
            width: 60px;
            height: 60px;
        }

        .sr-plate .sr-plate__text {
            font-size: 11px;
            line-height: 12px;
